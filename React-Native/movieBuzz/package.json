{
  "name": "moviebuzz",
  "version": "1.0.0",
  "description": "Your ultimate movie companion app built with React Native & Expo",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:preview": "eas build --platform android --profile preview",
    "build:production": "eas build --platform all --profile production",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react-native",
    "expo",
    "movies",
    "tmdb",
    "typescript",
    "mobile-app",
    "cinema",
    "entertainment"
  ],
  "author": {
    "name": "Mandar Vyas",
    "email": "your-email@example.com",
    "url": "https://github.com/mandarvyas"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mandarvyas/tech-practice-hub.git",
    "directory": "React-Native/movieBuzz"
  },
  "license": "MIT",
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "axios": "^1.7.7",
    "expo": "~51.0.28",
    "expo-av": "~14.0.7",
    "expo-build-properties": "~0.12.5",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
